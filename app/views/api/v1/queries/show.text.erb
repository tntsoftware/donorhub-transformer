[APPLICATION]
MinimumVersion=3.1.0
RecommendedVersion=3.2.5

[ORGANIZATION]
RedirectQueryIni=
Code=<%= current_organization.code %>
Name=<%= current_organization.name %>
Abbreviation=<%= current_organization.abbreviation %>
AccountHelpUrl=<%= current_organization.account_help_url %>
RequestProfileUrl=<%= current_organization.request_profile_url %>
OrgHelpUrl=<%= current_organization.help_url %>
OrgHelpUrlDescription=<%= current_organization.help_description %>
MinimumWebGiftDate=<%= current_organization.donations.order(:created_at).first&.created_at&.strftime('%m/%d/%Y') || '01/01/1990' %>
MinPidLength=
DefaultCurrencyCode=<%= current_organization.currency_code %>
GLAccountCategoriesToConsolidate=

[PROFILES]
Url=<%= api_v1_designation_profiles_url %>
Post=user_email=$ACCOUNT$&user_token=$PASSWORD$

[DESIGNATIONS]
Url=<%= api_v1_designation_accounts_url %>
Post=user_email=$ACCOUNT$&user_token=$PASSWORD$&designation_profile_id=$PROFILE$

[DONATIONS]
Url=<%= api_v1_donations_url %>
Post=user_email=$ACCOUNT$&user_token=$PASSWORD$&designation_profile_id=$PROFILE$&date_from=$DATEFROM$&date_to=$DATETO$

[ADDRESSES]
Url=<%= api_v1_donor_accounts_url %>
Post=user_email=$ACCOUNT$&user_token=$PASSWORD$&designation_profile_id=$PROFILE$&date_from=$DATEFROM$

[ADDRESSES_BY_PERSONIDS]
Url=<%= api_v1_donor_accounts_url %>
Post=user_email=$ACCOUNT$&user_token=$PASSWORD$&designation_profile_id=$PROFILE$&donor_account_ids=$PERSONIDS$

[ACCOUNT_BALANCE]
Url=<%= api_v1_balances_url %>
Post=user_email=$ACCOUNT$&user_token=$PASSWORD$&designation_profile_id=$PROFILE$
